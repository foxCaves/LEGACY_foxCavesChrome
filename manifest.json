{
	"name": "foxCaves",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Extension for foxCaves in your browser",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"page": "background.html",
		"persistent": false
	},
	"permissions": [
		"https://foxcav.es/",
		"https://fox.gy/",
		"<all_urls>",
		"tabs",
		"clipboardWrite",
		"contextMenus"
	]
}