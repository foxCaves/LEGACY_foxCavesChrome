{
  "name": "foxCaves",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Extension for foxCaves in your browser",
  "browser_action": {
    "default_icon": "icon.png",
	"default_popup": "popup.html"
  },
  "permissions": [
    "https://foxcav.es/",
	"http://foxcav.es/",
	"https://fox.gy/",
	"http://fox.gy/",
	"<all_urls>",
	"tabs"
  ]
}